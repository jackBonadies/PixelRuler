<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="SeperatorColor">#2D2D2D</SolidColorBrush>
    <SolidColorBrush x:Key="PanelColor">#202020</SolidColorBrush>
    <SolidColorBrush x:Key="TextFillColorStrongBrush">#ffffffff</SolidColorBrush>
    <SolidColorBrush x:Key="HoverOverLight">#18ffffff</SolidColorBrush>
    <SolidColorBrush x:Key="PressedLight">#30ffffff</SolidColorBrush>
    <SolidColorBrush x:Key="ColorBorderColor">#33ffffff</SolidColorBrush>
    <SolidColorBrush x:Key="ToolPanelBackground" Color="#303030" />
    <SolidColorBrush x:Key="ToolPanelBorder" Color="#606060" />
    <SolidColorBrush x:Key="GreenSuccess" Color="#29D45B" />
    <Color x:Key="StrongErrorColor">#FF5555</Color>
    <SolidColorBrush x:Key="StrongErrorBrush" Color="{DynamicResource StrongErrorColor}" />

    <Color x:Key="SubtleMouseOverColor">#10ffffff</Color>
    <Color x:Key="SubtleMousePressedColor">#20ffffff</Color>
    <SolidColorBrush x:Key="SubtleMouseOverBrush" Color="{StaticResource SubtleMouseOverColor}" />
    <SolidColorBrush x:Key="SubtleMousePressedBrush" Color="{StaticResource SubtleMousePressedColor}" />
</ResourceDictionary>