<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="SeperatorColor">#22000000</SolidColorBrush>
    <SolidColorBrush x:Key="TextFillColorStrongBrush">#ff000000</SolidColorBrush>
    <SolidColorBrush x:Key="HoverOverLight">#18000000</SolidColorBrush>
    <SolidColorBrush x:Key="PressedLight">#40000000</SolidColorBrush>
    <SolidColorBrush x:Key="PanelColor">#EFF4F9</SolidColorBrush>
    <SolidColorBrush x:Key="ToolPanelBackground" Color="White" />
    <SolidColorBrush x:Key="ToolPanelBorder" Color="Gray" />
    <SolidColorBrush x:Key="ColorBorderColor">#aa303030</SolidColorBrush>
    <SolidColorBrush x:Key="GreenSuccess" Color="#187A35" />


    <Color x:Key="StrongErrorColor">#FF0000</Color>
    <SolidColorBrush x:Key="StrongErrorBrush" Color="{DynamicResource StrongErrorColor}" />
    <Color x:Key="SubtleMouseOverColor">#e0e0e0</Color>
    <Color x:Key="SubtleMousePressedColor">#d0d0d0</Color>
    <SolidColorBrush x:Key="SubtleMouseOverBrush" Color="{StaticResource SubtleMouseOverColor}" />
    <SolidColorBrush x:Key="SubtleMousePressedBrush" Color="{StaticResource SubtleMousePressedColor}" />
</ResourceDictionary>